--Socket--
identifiers{
    8[0] = emptyness
    8[1] = playerobject

    
    8[5] = heartbeat/ping
    8[6] = chatmessage
}

playerdata= {1, x1, x2, x3, x4, y1, y2, y3, y4, sx1, sx2, sx3, sx4, sy1, sy2, sy3, sy4} = 17

udp:
tick{
    1024[x32[posx]y32[posy]sizex32[sizex]sizey32[sizey]]
}

tcp:
start{
    1024[password]
}
secondtick{
    1024[chatmessage1024[stringmessage]]
}


